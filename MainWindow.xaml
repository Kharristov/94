<Window x:Class="WpfApp5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="NamesTextBox" Width="300" Height="30" VerticalAlignment="Top" Margin="50,50,50,0"/>

        <Button Content="Показать лайки" Width="150" Height="30" VerticalAlignment="Top" Margin="125,100,125,0" Click="OnLikesClick"/>

        <TextBlock Name="ResultTextBlock" Width="300" Height="30" VerticalAlignment="Top" Margin="50,150,50,0" TextAlignment="Center"/>
    </Grid>
</Window>
